// See https://github.com/jshint/jshint/blob/2.x/examples/.jshintrc
{
    "globals": {
      "window": true,
      "document": true,
      /* MOCHA */
      "mocha"      : false,
      "describe"   : false,
      "it"         : false,
      "before"     : false,
      "beforeEach" : false,
      "after"      : false,
      "afterEach"  : false
    },
    "indent": 2,
    "node": true,
    "bitwise": true,
    "camelcase": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "smarttabs": false,
    "forin": false,
    "browser": false,
    "globalstrict": false,
    "esnext": false

}
